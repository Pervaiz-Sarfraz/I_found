<%- include('./includes/head.ejs') %>


<body data-spy="scroll" data-target=".fixed-top">

    <%- include('./includes/navbar.ejs') %>



    <div class="hero flex-column">
        <div class="post-creation">
            <div class="heading-post">
                <h1>found Items</h1>
                <p>This page will contain all the post of found items.if you have   found any thing click the
                    create post and post it and we may able to help you along</p>
                    <a class=" btn creat-post" href="/found">Create Post </a>

            </div>   
        </div>
        <div class="container">
            <div class="row">
                <% items.forEach(item=>{ %>
                <div class=" col-md-4 col-sm-6 col-12  mb-2">
                    <div class="card">
                        <div class="card-body">
                            <% if(item.img){ %> 
                                <img class="item-img" src="<%= item.img %>" alt="">
                                <% } %> 
                                <h5 class="card-title mt-3"><%= item.title %> </h5>
                                <p class="card-text m-0"><%= item.name %> </p>
                                <p class="card-text m-0"><%= item.email %> </p>
                                <p class="card-text m-0"><%= item.regNo %> </p>
                                <p class="card-text m-0"><%= item.dept %> </p>
                                <p class="card-text m-0"><%= item.properties %></p>
    
                        </div>
                    </div> 
                </div>
                <% }) %>
            </div>
        </div>
    </div>

